  function _makeRequest(currentRetryCount) {
    var request = new XMLHttpRequest();
    var asyncRequest = true;
    var SUCCESS = 200;
    var ERROR = 400;

    request.open('GET', '/globalcampus/exchange/nav_item', asyncRequest);

    var headers = {};
